                "examples": [
                    "Cheetah"
                ]
            },
            "taxonomy": {
                "type": "object",
                "default": {},
                "title": "The taxonomy Schema",
                "required": [
                    "scientific_name"
                ],
                "properties": {
                    "phylum": {
                        "type": "string",
                        "default": "",
                        "title": "The phylum Schema",
                        "examples": [
                            "Chordata"
                        ]
                    },
                    "class": {
                        "type": "string",
                        "default": "",
                        "title": "The class Schema",
                        "examples": [
                            "Mammalia"
                        ]
                    },
                    "scientific_name": {
                        "type": "string",
                        "default": "",
                        "title": "The scientific_name Schema",
                        "examples": [
                            "Acinonyx jubatus"
                        ]
                    }
                },
                "examples": [{
                    "phylum": "Chordata",
                    "class": "Mammalia",
                    "scientific_name": "Acinonyx jubatus"
                }]
            },
            "locations": {
                "type": "array",
                "default": [],
                "title": "The locations Schema",
                "items": {
                    "type": "string",
                    "title": "A Schema",
                    "examples": [
                        "Africa",
                        "Asia",
                        "Eurasia"
                    ]
                },
                "examples": [
                    ["Africa",
                        "Asia",
                        "Eurasia"
                    ]
                ]
            },
            "characteristics": {
                "type": "object",
                "default": {},
                "title": "The characteristics Schema",
                "required": [
                    "common_name"
                ],
                "properties": {
                    "prey": {
                        "type": "string",
                        "default": "",
                        "title": "The prey Schema",
                        "examples": [
                            "Gazelle, Wildebeest, Hare"
                        ]
                    },
                    "most_distinctive_feature": {
                        "type": "string",
                        "default": "",
                        "title": "The most_distinctive_feature Schema",
                        "examples": [
                            "Yellowish fur covered in small black spots"
                        ]
                    },
                    "habitat": {
                        "type": "string",
                        "default": "",
                        "title": "The habitat Schema",
                        "examples": [
                            "Open grassland"
                        ]
                    },
                    "diet": {
                        "type": "string",
                        "default": "",
                        "title": "The diet Schema",
                        "examples": [
                            "Carnivore"
                        ]
                    },
                    "common_name": {
                        "type": "string",
                        "default": "",
                        "title": "The common_name Schema",
                        "examples": [
                            "Cheetah"
                        ]
                    },
                    "location": {
                        "type": "string",
                        "default": "",
                        "title": "The location Schema",
                        "examples": [
                            "Asia and Africa"
                        ]
                    },
                    "slogan": {
                        "type": "string",
                        "default": "",
                        "title": "The slogan Schema",
                        "examples": [
                            "The fastest land mammal in the world!"
                        ]
                    }
                },
                "examples": [{
                    "prey": "Gazelle, Wildebeest, Hare",
                    "most_distinctive_feature": "Yellowish fur covered in small black spots",
                    "habitat": "Open grassland",
                    "diet": "Carnivore",
                    "common_name": "Cheetah",
                    "location": "Asia and Africa",
                    "slogan": "The fastest land mammal in the world!"
                }]
            }
        },
        "examples": [{
